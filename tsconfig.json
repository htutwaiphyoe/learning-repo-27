{
  "compilerOptions": {
    "jsx": "react-jsx",
    "composite": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.node.tsbuildinfo",
    "skipLibCheck": true,
    "target": "ES5",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "noEmit": true,
    "typeRoots": ["./dist/index.d.ts", "node_modules/@types"]
  },
  "include": ["vite.config.ts", "src"]
}
